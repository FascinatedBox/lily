define fix_size(input: Integer): Integer
{
    var result = input

    if bench.is_quiet: {
        result = result / 4
    }

    return result
}

define bench_test
{
    var keys: List[String] = []
    var animal_size    = animals.size()    |> fix_size
    var adjective_size = adjectives.size() |> fix_size
    var adverb_size    = adverbs.size()    |> fix_size
    var map: Hash[String, Integer] = []
    var i = 0
    var sum = 0
    var start = bench.start()

    for i in 0...animal_size - 1: {
        for j in 0...adjective_size - 1: {
            for k in 0...adverb_size - 1: {
                var f = "{0} {1} {2}".format(adverbs[k], adjectives[j], animals[i])
                keys.push(f)
            }
        }
    }

    keys.each(|key|
        map[key] = i
        i += 1 )

    keys.each(|key| sum += map[key] )
    keys.each(|key| map.delete(key) )

    bench.log(sum)
    bench.finish(start)
}

bench.run(bench_test)

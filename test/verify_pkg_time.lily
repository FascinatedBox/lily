import test

var t = test.t

t.scope(__file__)

import time

t.assert("Time.now returns a time value.",
         (||
    var tm = time.Time.now()
    true ))

t.assert("Time.clock returns seconds.",
         (||
    time.Time.clock()
    true ))

t.assert("Time.since_epoch returns Integer.",
         (||
    var tm = time.Time.now()
    tm.since_epoch()
    true ))

t.assert("Time.to_s on a Time value.",
         (||
    var tm = time.Time.now()
    tm.to_s()
    true ))

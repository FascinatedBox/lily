import test

var t = test.t

t.scope(__file__)

t.assert("Integer.to_bool for 1.",
         (|| 1.to_bool() == true ))

t.assert("Integer.to_bool for 0.",
         (|| 0.to_bool() == false ))

t.assert("Integer.to_bool for -1.",
         (|| -1.to_bool() == true ))

t.assert("Integer.to_byte for 0.",
         (|| 0.to_byte() == 0t ))

t.assert("Integer.to_byte for 255.",
         (|| 255.to_byte() == 255t ))

t.assert("Integer.to_d for 500.",
         (|| 500.to_d() == 500.0 ))

t.assert("Integer.to_d for -250.",
         (|| -250.to_d() == -250.0 ))

t.assert("Integer.to_d for 0.",
         (|| 0.to_d() == 0.0 ))

# fibonacci numbers
class Fib {
    public var @a=0
    public var @b=1
    public define next: Integer {
        var c = @a + @b
        @a = @b
        @b = c
        return @a
    }
}

var fib = Fib()
print([fib.next(),
       fib.next(),
       fib.next(),
       fib.next(),
       fib.next(),
       fib.next()])
